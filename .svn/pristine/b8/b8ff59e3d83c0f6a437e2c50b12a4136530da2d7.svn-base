
body, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, blockquote, th, td, hr, button, article, aside, details, figcaption, figure, footer, header, group, menu, nav, section {
    margin: 0;
    padding: 0;
}


button, input {
    line-height: normal;
}
button {
    cursor: pointer;
}
button::-moz-focus-inner, input::-moz-focus-inner {
    border: 0 none;
    padding: 0;
}

blockquote {
    background: none repeat scroll 0 0 #F1F1F1;
    margin: 1em 4em 1em 2em;
    padding: 0.6em 1em;
}
blockquote blockquote {
    border-left: 0.4em solid #DDDDDD;
    margin-left: 2em;
    padding: 0 0 0 1em;
}

.grid {
    margin: 0 auto;
    width: 990px;
}
.grid:before, .grid:after {
    content: "";
    display: table;
}
.grid:after {
    clear: both;
}


.page {
    padding: 10px 0;
}
.page .week-topic {
    border: 1px solid #666666;
    height: 400px;
    margin-bottom: 20px;
    overflow: hidden;
    width: 990px;
}
.page .week-topic .topic {
    background: none repeat scroll 0 0 lightSlateGray;
    color: white;
    font-size: 14px;
    line-height: 25px;
    text-align: center;
}
.page .week-topic .slider-container {
    height: 400px;
    overflow: hidden;
    position: relative;
    width: 990px;
}
.page .week-topic .slider-container img {
    height: 400px;
    width: 990px;
}
.page .week-topic .slider-controls {
    bottom: 10px;
    position: absolute;
    right: 5px;
}
.page .week-topic .slider-controls li {
    -moz-border-radius: 2px 2px 2px 2px;
    background: none repeat scroll 0 0 #000000;
    color: #FFFFFF;
    display: block;
    float: left;
    height: 20px;
    line-height: 20px;
    margin-right: 5px;
    text-align: center;
    text-decoration: none;
    width: 20px;
}
.page .week-topic .slider-controls li:hover, .page .week-topic .slider-controls .hover, .page .week-topic .slider-controls .selected {
    background: none repeat scroll 0 0 rgba(153, 51, 0, 0.8);
}
.page .week-topic .slider-content {
    background: none repeat scroll 0 0 rgba(200, 200, 200, 0.6);
    bottom: 0;
    color: #333333;
    left: 0;
    position: absolute;
    text-shadow: 1px 1px 1px #999999;
    width: 100%;
}
.page .week-topic .slider-content dt, .page .week-topic .slider-content dd {
    padding: 10px;
}
.page .module {
    margin-right: -30px;
}
.page .module .list {
    -moz-box-shadow: 0 0 3px #666666;
    background: none repeat scroll 0 0 #F1F1F1;
    float: left;
    height: 500px;
    margin-bottom: 20px;
    margin-right: 30px;
    position: relative;
    width: 310px;
}
.page .module .list:hover {
    -moz-box-shadow: 0 0 10px #333333;
}
.page .module .list:hover .title {
    background: none repeat scroll 0 0 transparent;
    text-shadow: 0 0 5px #333333;
}
.page .module .list .logo {
    display: block;
    height: 200px;
    width: 310px;
}
.page .module .list .title {
    background: none repeat scroll 0 0 rgba(50, 50, 50, 0.8);
    color: #FFFFFF;
    font-size: 36px;
    height: 200px;
    left: 0;
    line-height: 200px;
    position: absolute;
    text-align: center;
    text-shadow: 0 0 5px #333333;
    top: 0;
    width: 310px;
}
.page .module .list .title a {
    color: #FFFFFF;
    display: block;
    text-decoration: none;
}
.page .module .list .info .author {
    border-bottom: 1px solid #DDDDDD;
    height: 30px;
    line-height: 30px;
    padding: 0 10px;
}
.page .module .list .info .author a {
    color: #333333;
    font-weight: bold;
}
.page .module .list .info .author .demo {
    float: right;
}
.page .module .list .info .desription {
    border-bottom: 1px solid #DDDDDD;
    border-top: 1px solid white;
    height: 230px;
    overflow: hidden;
    padding: 5px 10px;
}
.page .module .list .info .download {
    border-top: 1px solid white;
    padding: 3px 10px;
}
.page .module .list .info .download .icon-down {
    background: url("../img/download.png") no-repeat scroll 50% 50% transparent;
    display: inline-block;
    height: 16px;
    margin-right: 5px;
    vertical-align: middle;
    width: 16px;
}
.page .module .list .info .download .icon-bug {
    background: url("../img/bugs.png") no-repeat scroll 50% 50% transparent;
    display: inline-block;
    height: 16px;
    margin-right: 5px;
    vertical-align: middle;
    width: 16px;
}
.page .module .list .info .download a {
    margin-right: 10px;
    text-decoration: none;
}

