			//win vista/7 + flash plugin 11.3 + ff 4-13 == problem
			if(flash && flash.has && nav.oscpu && FOCUS.util.browser.firefox && FOCUS.util.browser.version >= 4 && FOCUS.util.browser.version <= 13){
